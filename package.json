{
  "name": "xstream-combine-unique",
  "version": "1.0.0",
  "description": "An xstream operator that combines two streams using each event at most once.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "test": "mocha src/*.test.js --watch",
