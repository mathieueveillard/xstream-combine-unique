    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieueveillard/xstream-combine-unique.git"
  },
  "keywords": [
    "xstream",
    "rxjs",
    "observables",
    "operator",
    "combine",
    "unique",
    "typescript",
    "javascript",
    "npm-package"
  ],
  "author": "Mathieu Eveillard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mathieueveillard/xstream-combine-unique/issues"
  },
  "homepage": "https://github.com/mathieueveillard/xstream-combine-unique#readme",
  "dependencies": {
    "xstream": "^11.7.0"
  },
  "devDependencies": {
    "@cycle/time": "^0.15.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "mocha": "^5.2.0",
    "typescript": "^3.0.1"
  }
